                                                                                                <label class="mb-1 text-white fs-14">Amount <span class="text-danger">*</span></label>
                                                                                                @Html.TextBoxFor(m => m.WithDrawAmount, new { @class = "form-control", @Placeholder = "Amount", @onkeypress = "return isNumberKey(event);" })
