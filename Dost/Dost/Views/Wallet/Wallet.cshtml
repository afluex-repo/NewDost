                                                                                                <label class="mb-1 text-white fs-14">Branch <span class="text-danger">*</span></label>
                                                                                                @Html.TextBoxFor(m => m.BankBranch, new { @class = "form-control", @placeholder = "Bank Branch" })
                                                                                            </div>
                                                                                        </div>

