@model Dost.Models.Wallet

@{
    ViewBag.Title = "UsedPins";
    Layout = "~/Views/Shared/_AdminLayout.cshtml";
}

<div class="row">
    <div class="col-sm-12">

        <ol class="breadcrumb">
            <li>
                <i class="clip-home-3"></i>
                <a href="#">
                    Coupon Management
                </a>
            </li>
            <li class="active">
                Used Coupon
            </li>

        </ol>


    </div>
</div>
<br />
@using (Html.BeginForm("UsedPins", "Admin", FormMethod.Post, new { @id = "form1", name = "formKYC" }))
{

    <div class="row">
        <div class="col-lg-12">
            <div class="panel panel-default">
                <div class="panel-heading">
                    Used Coupon


                </div>
                <!-- /.panel-heading -->
                <div class="panel-body">
                    <div class="row">
                        <div class="col-lg-4">
                            <div class="form-group">
                                <label>Coupon Code </label>
                                @Html.TextBoxFor(m => m.CouponCode, new { @class = "form-control" })
                            </div>
                        </div>
                        <div class="col-lg-4">
                            <div class="form-group">
                                <label>Coupon User </label>
                                @Html.TextBoxFor(m => m.OwnerName, new { @class = "form-control", @disable = true })
                            </div>
                        </div>

                        <div class="col-lg-4">
                            <div class="form-group">
                                <label>Category  </label>
                                @Html.DropDownListFor(m => m.CategoryCode, new SelectList(ViewBag.ddlProduct, "Value", "Text"), new { @class = "form-control", @onchange = "FillAmount();" })
                            </div>
                        </div>

                    </div>
                    <div class="row">
                        <div class="col-lg-3">
                            <div class="form-group">
                                <br />
                                <input type="submit" id="btnSearch" class="btn btn-success pull-left" name="Search" value="Search" />
                            </div>
                        </div>
                    </div>
                   
                 <br/>
                    <table id="example23" class="display nowrap table table-hover table-striped table-bordered" cellspacing="0" width="100%">
                        <thead>
                            <tr>
                                <th>Sr No.</th>
                                <th>Coupon Owner</th>
                                <th>Coupon Code</th>
                                <th>Category</th>
                                <th>Event/Plan/NFC</th>
                                <th>Created Date</th>
                                <th>Coupon Price</th>
                                <th>Registered To</th>
                                <th>Used Date</th>
                                <th>Status</th>


                            </tr>
                        </thead>
                        <tbody>
                            @if (Model != null)
                            {
                                if (Model.lstunusedpins != null)
                                {
                                    int i = 1;
                                    foreach (var Item in Model.lstunusedpins)
                                    {
                                        <tr class="text-left">
                                            <td>@i</td>
                                            <td>@Item.OwnerName</td>
                                            <td>@Item.CouponCode</td>
                                            <td>@Item.CategoryName</td>
                                            <td>@Item.EventName</td>
                                            <td>@Item.CreatedDate</td>
                                            <td>@Item.CouponPrice</td>
                                            <td>@Item.RegisteredTo</td>
                                            <td>@Item.TopUpDate</td>
                                            <td>@Item.CuopnStatus</td>

                                        </tr>
                                        i++;
                                    }

                                }
                            }

                        </tbody>
                    </table>
              </div>
                </div>
            </div>
        </div>
}


<script src="../vendor/jquery/jquery.min.js"></script>