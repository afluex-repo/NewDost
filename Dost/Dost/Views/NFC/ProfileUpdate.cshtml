    var ProfileId = "";
    $('input[name="ProfileType"]').on('change', function () {
        $('input[name="ProfileType"]').not(this).prop('checked', false);
    });
    $("#addBtn").click(function () {
        $("#hdfProfile").val('');
    })
    $("#btnProfileAdd").click(function () {
        debugger;
        if (window.FormData !== undefined) {
            var fileData = new FormData();
            fileData.append('Flag', 'Profile');
            fileData.append('ProfileName', $("#ProfileName").val());
            fileData.append('ProfileType', $('input[name="ProfileType"]:checked').val());
            $.ajax({
                url: '/nfc/ProfileUpdate',
                type: "POST",
                contentType: false,
                processData: false,
                data: fileData,
                success: function (data) {
                    $("#Flag").val('');
                    if (data.Result == "Success") {
                        toastr.success(data.Message, "", {
                            timeOut: 500000000,
                            closeButton: !0,
                            debug: !1,
                            newestOnTop: !0,
                            progressBar: !0,
                            positionClass: "toast-top-right",
                            preventDuplicates: !0,
                            onclick: null,
                            showDuration: "300",
                            hideDuration: "1000",
                            extendedTimeOut: "1000",
                            showEasing: "swing",
                            hideEasing: "linear",
                            showMethod: "fadeIn",
                            hideMethod: "fadeOut",
                            tapToDismiss: !1
                        })
                    }
                    else {
                        toastr.error(data.Message, "", {
                            positionClass: "toast-top-right",
                            timeOut: 5e3,
                            closeButton: !0,
                            debug: !1,
                            newestOnTop: !0,
                            progressBar: !0,
                            preventDuplicates: !0,
                            onclick: null,
                            showDuration: "300",
                            hideDuration: "1000",
                            extendedTimeOut: "1000",
                            showEasing: "swing",
                            hideEasing: "linear",
                            showMethod: "fadeIn",
                            hideMethod: "fadeOut",
                            tapToDismiss: !1
                        })
                    }
                    GetProfilePersonalData();
                    $('#exampleModalCenter').modal('hide');
                },
                error: function (err) {
                    toastr.error("Error while inserting data", "", {
                        positionClass: "toast-top-right",
                        timeOut: 5e3,
                        closeButton: !0,
                        debug: !1,
                        newestOnTop: !0,
                        progressBar: !0,
                        preventDuplicates: !0,
                        onclick: null,
                        showDuration: "300",
                        hideDuration: "1000",
                        extendedTimeOut: "1000",
                        showEasing: "swing",
                        hideEasing: "linear",
                        showMethod: "fadeIn",
                        hideMethod: "fadeOut",
                        tapToDismiss: !1
                    })
                    GetProfilePersonalData();
                    $('#exampleModalCenter').modal('hide');
                }
            });
        }
        $("#Flag").val("Profile");
    });

    function GetProfilePersonalData() {
        debugger;
        $.ajax({
            type: 'Get',
            contentType: "Application/Json",
            url: '@Url.Action("GetProfilePersonalData", "NFC")',
            dataType: 'json',
            success: function (data) {
                debugger;
                $("#divbindProfile").html('');
                var obj = jQuery.parseJSON(data);
                for (var i = 0; i < obj.length; i++) {
                    $("#divbindProfile").append('<div class="swiper-slide"><div class="card-bx stacked card"><img src="../images/card/card1.jpg" alt=""><div class="card-info"><p class="mb-1 text-white fs-14">' + obj[i].ProfileName + '</p><div class="d-flex justify-content-between"><h2 class="num-text text-white mb-5 font-w600">' + obj[i].Name + '</h2><div class="text-center"><span class="num-text text-white mb-5 font-w600"></span><div class="mt-4 text-white"><input type="checkbox" class="chkactive" value=' + obj[i].PK_ProfileId + ' ' + obj[i].IsChecked + '/><p>Activate</p><p><a href="edit_contact_card.html" class="text-white"><i class="fa fa-edit"></i> Edit</a></p></div></div></div><div class="d-flex"><div class="mb-2"><button type="button" class="btn btn-secondary" onclick="GetBusinessProfile(' + obj[i].PK_ProfileId + ');">' + obj[i].ProfileType + '</button></div></div></div></div></div>')
                }
            }
        });
    }
    $("#divbindProfile").on("click", ".chkactive", function (e) {
        if ($(this).prop("checked") == true) {
            IsChecked = true;
        }
        else if ($(this).prop("checked") == false) {
            IsChecked = false;
        }
        $('#divbindProfile .chkactive input:checkbox').not(this).prop('checked', false);
        $('#divbindProfile .chkactive').change(function () {
            debugger;
            ProfileId = $(this).val();
            $.ajax({
                type: "GET",
                dataType: "JSON",
                contentType: "Application/Json",
                url: '@Url.Action("UpdateProfileStatus", "nfc")',
                data: { ProfileId: ProfileId, IsChecked: IsChecked },
                success: function (data) {
                    if (data.Result == "Yes") {
                        toastr.success("Profile status changed", "", {
                            timeOut: 500000000,
                            closeButton: !0,
                            debug: !1,
                            newestOnTop: !0,
                            progressBar: !0,
                            positionClass: "toast-top-right",
                            preventDuplicates: !0,
                            onclick: null,
                            showDuration: "300",
                            hideDuration: "1000",
                            extendedTimeOut: "1000",
                            showEasing: "swing",
                            hideEasing: "linear",
                            showMethod: "fadeIn",
                            hideMethod: "fadeOut",
                            tapToDismiss: !1
                        });
                        $("#hdfProfile").val(ProfileId);
                        GetProfilePersonalData();
                    }
                    else {
                        toastr.error("Please try again", "", {
                            timeOut: 500000000,
                            closeButton: !0,
                            debug: !1,
                            newestOnTop: !0,
                            progressBar: !0,
                            positionClass: "toast-top-right",
                            preventDuplicates: !0,
                            onclick: null,
                            showDuration: "300",
                            hideDuration: "1000",
                            extendedTimeOut: "1000",
                            showEasing: "swing",
                            hideEasing: "linear",
                            showMethod: "fadeIn",
                            hideMethod: "fadeOut",
                            tapToDismiss: !1
                        });
                    }
                }
            });
        });
    });
    window.GetBusinessProfile = function (Id) {
        $.ajax({
            type: "GET",
            dataType: "JSON",
            contentType: "Application/Json",
            url: '@Url.Action("GetBusinessProfile", "nfc")',
            data: { PK_ProfileId: Id },
            success: function (data) {

                if (data != null) {
                    var obj = jQuery.parseJSON(data);
                    if (obj[0].PK_ProfileId != null) {
                        $("#lblName").val(obj[0].FirstName);
                        $("#FirstName").val(obj[0].FirstName);
                        $("#hdfProfile").val(obj[0].PK_ProfileId);
                        $("#LastName").val(obj[0].LastName);
                        $("#BusinessName").val(obj[0].BusinessName);
                        $("#Designation").val(obj[0].Designation);
                        $("#Description").val(obj[0].Description);
                        $("#MyProfileImage").attr("src", obj[0].ProfilePic);
                    }
                    $("#btnBusiness").val('Update');
                    $("#divbusiness1").show();
                    status.fadeOut();
                    preloader.delay(0).fadeOut('fast');
                }
            }
        });
    }

</script>